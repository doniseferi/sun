{
  "name": "suntimes",
  "version": "4.0.0-3",
  "description": "A simple library to get the exact date and time in utc for certain points of the day for any desired location and date.",
  "main": "lib/suntimes.js",
  "module": "src/index",
  "scripts": {
    "bundle": "rimraf lib && rollup -c",
    "test": "semistandard && nyc mocha --opts support/mocha.opts **/*.test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "fix": "semistandard --fix",
    "pack": "npm run bundle && copyfiles README.md package.json License.md lib",
    "release": "npm run pack && np --contents=lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doniseferi/suntimes.git"
  },
  "keywords": [
    "sun",
    "suntimes",
    "equationOfTime",
    "solar",
    "declination",
    "sunrise",
    "sunset",
    "twilight",
    "dusk",
    "dawn",
    "maths",
    "astronomy",
    "celestial"
  ],
  "author": "Edonis Seferi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doniseferi/suntimes/issues"
  },
  "homepage": "https://github.com/doniseferi/suntimes#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "chai": "^4.2.0",
    "copyfiles": "^2.1.1",
    "coveralls": "^3.0.7",
    "depcheck": "^0.9.0",
    "eslint": "^6.6.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc-to-markdown": "^5.0.2",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "semistandard": "^14.2.0"
  }
}
