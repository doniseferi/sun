{
  "name": "suntimes",
  "version": "3.0.1",
  "description": "A simple library to get the exact date and time in utc for certain points of the day for any desired location and date.",
  "main": "dist/index.js",
  "scripts": {
    "test": "semistandard && nyc mocha --opts support/mocha.opts **/*.test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "fix": "semistandard --fix",
    "build": "babel src --out-dir dist --delete-dir-on-start --ignore \"src/**/*.test.js\",\"src/**/test\" && copyfiles package.json License.md README.md logo.svg dist/",
    "release": "npm run build && np"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doniseferi/suntimes.git"
  },
  "keywords": [
    "sun",
    "suntimes",
    "equationOfTime",
    "solar",
    "declination",
    "sunrise",
    "sunset",
    "twilight",
    "dusk",
    "dawn",
    "maths",
    "astronomy",
    "celestial"
  ],
  "author": "Edonis Seferi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doniseferi/suntimes/issues"
  },
  "homepage": "https://github.com/doniseferi/suntimes#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "chai": "^4.2.0",
    "copyfiles": "^2.1.1",
    "coveralls": "^3.0.7",
    "depcheck": "^0.9.0",
    "eslint": "^6.6.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc-to-markdown": "^5.0.2",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "semistandard": "^14.2.0"
  }
}
