{
  "name": "suntimes",
  "version": "5.0.0-0",
  "description": "A tiny library to get the exact date and time in utc for certain points of the day for any desired location and date.",
  "main": "suntimes.js",
  "typings": "suntimes.d.ts",
  "scripts": {
    "readme": "jsdoc2md ./src/index.js",
    "typedef": "jsdoc -t node_modules/tsd-jsdoc/dist -r ./src/index.js",
    "build": "rollup -c",
    "release": "standard-version",
    "test": "semistandard && npm run build && nyc mocha **/*.test.js --colors --full-trace --require @babel/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "fix": "semistandard --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doniseferi/suntimes.git"
  },
  "keywords": [
    "sun",
    "suntimes",
    "equationOfTime",
    "solar",
    "declination",
    "sunrise",
    "sunset",
    "twilight",
    "dusk",
    "dawn",
    "maths",
    "astronomy",
    "celestial"
  ],
  "author": "Edonis Seferi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doniseferi/suntimes/issues"
  },
  "homepage": "https://github.com/doniseferi/suntimes#readme",
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "depcheck": "^0.9.1",
    "eslint": "^6.6.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc": "^3.6.3",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^7.1.1",
    "nyc": "^14.1.1",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "semistandard": "^14.2.0",
    "standard-version": "^7.1.0",
    "tsd-jsdoc": "^2.4.0"
  },
  "files": [
    "suntimes.d.ts"
  ],
  "dependencies": {}
}
